; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Panel",
"class": "IsoPanel",
"language": "GDScript",
"path": "res://SingleTextureContainer/SingleTextureContainer.gd"
}, {
"base": "Panel",
"class": "ResultsPanel",
"language": "GDScript",
"path": "res://SingleTextureContainer/ResultsPanel.gd"
}, {
"base": "TextureButton",
"class": "TexturePreview",
"language": "GDScript",
"path": "res://Components/Preview.gd"
}, {
"base": "PanelContainer",
"class": "TextureTray",
"language": "GDScript",
"path": "res://Components/TextureTray.gd"
} ]
_global_script_class_icons={
"IsoPanel": "",
"ResultsPanel": "",
"TexturePreview": "",
"TextureTray": ""
}

[application]

config/name="Isometric Tileset Generator"
run/main_scene="res://Workspace.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true

[rendering]

environment/default_clear_color=Color( 0.14902, 0.14902, 0.14902, 1 )
environment/default_environment="res://default_env.tres"
